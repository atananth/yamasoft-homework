{
  "name": "yamasoft-homework",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "pnpm run sass-dev & vite --port 4175",
    "prod": "pnpm run sass-prod && pnpm run build && vite preview",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "e2e": "pnpm run dev; cypress run --browser chromium --spec cypress/e2e/trips.cy.js",
    "sass-dev": "sass --watch --update --style=expanded src/assets/scss:src/assets/css",
    "sass-prod": "sass --no-source-map --style=compressed src/assets/scss:src/assets/css"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.8",
    "@mui/lab": "7.0.1-beta.22",
    "@mui/material": "^7.3.8",
    "mui-image": "^1.0.9",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-error-boundary": "^6.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/mui-image": "^1.0.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "cypress": "^15.10.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "sass": "^1.97.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^8.0.0-beta.13"
  },
  "pnpm": {
    "overrides": {
      "vite": "^8.0.0-beta.13"
    },
    "onlyBuiltDependencies": [
      "cypress"
    ]
  }
}
